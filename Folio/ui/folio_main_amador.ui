                      <property name="styleSheet">
                       <string notr="true">/* General QDateEdit styling */
QDateTimeEdit {
    background-color: rgb(0, 189, 189);
	color: rgb(255, 255, 255);
    border-radius: 15px; /* Rounded corners */
    padding: 5px 10px; /* Inner padding */
	font: 12pt &quot;Arial&quot;;
}

/* Arrow buttons for spin up/down */
QDateTimeEdit::up-button, QDateTimeEdit::down-button {
    border: none; /* Remove button borders */
    background-color: #3B82F6; /* Highlighted blue background */
    border-radius: 4px; /* Rounded arrow buttons */
    width: 8px; /* Set size */
    height: 8px;
    margin: 2px; /* Spacing */
}

QDateTimeEdit::up-button:hover, QDateTimeEdit::down-button:hover {
    background-color: #4C8EF7; /* Lighter blue on hover */
}

/* Default arrows for up and down */
QDateTimeEdit::up-arrow, QDateTimeEdit::down-arrow {
    width: 10px;
    height: 10px;
}

/* Disabled state for date input */
QDateTimeEdit:disabled {
    background-color: #444B56; /* Darker gray */
    color: #888888; /* Dimmed text */
    border: 1px solid #555555;
}
