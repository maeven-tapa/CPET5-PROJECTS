QCalendarWidget {
    background-color: rgb(0, 234, 234) /* Dark background */
    border: 1px solid rgb(0, 189, 189);
    border-radius: 10px;
    color: #FFFFFF;
    font-family: Arial;
    font-size: 12px;
}

/* Header styling for month and year */
QCalendarWidget QToolButton {
    background-color: rgb(0, 189, 189);
    color: rgb(0, 189, 189)
    font-weight: bold;
    border: none;
    border-radius: 5px;
    padding: 5px;
}

QCalendarWidget QToolButton:hover {
    background-color: rgb(0, 234, 234);
}

QCalendarWidget QToolButton#qt_calendar_prevmonth, 
QCalendarWidget QToolButton#qt_calendar_nextmonth {
    background:rgb(0, 189, 189);
    qproperty-icon: url(:/icons/arrow.png);  /* Add custom arrow icons */
}

/* Navigation bar (month and year) */
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: rgb(0, 234, 234);
    border-bottom: 1px solid rgb(252, 2, 128);
    color: white;
    font-size: 14px;
}

/* Week numbers */
QCalendarWidget QHeaderView {
    background-color: rgb(0, 173, 173);
    border: none;
    color: rgb(0, 158, 158);
}

/* Day names */
QCalendarWidget QTableView {
    alternate-background-color:  rgb(255, 255, 255);;
    color:rgb(0, 189, 189);
}

/* Day cells */
QCalendarWidget QAbstractItemView:enabled {
	background-color: rgb(0, 236, 236);
    color: rgb(0, 189, 189);
    selection-background-color: rgb(0, 135, 135);  /* Highlight color */
    selection-color: white;
    border-radius: 20px;
border: 5px solid  rgb(252, 2, 128);
}

/* Today Highlight */
QCalendarWidget QAbstractItemView:enabled:selected {
    background-color:rgb(0, 189, 189);
    color: rgb(200, 12, 31)
}

QCalendarWidget QAbstractItemView:enabled:focus {
    border: 1px solid #4E7AC7;
}

/* Disable previous/next month dates */
QCalendarWidget QAbstractItemView:disabled {
    color: #666666;
}

/* Circle the available dates manually */
QCalendarWidget QTableView::item:selected {
    border: 1px solid white;
    border-radius: 10px;  /* Circular style */
}
